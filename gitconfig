[user]
	name = Cameron Eagans
	email = me@cweagans.net
[push]
	default = simple
[core]
	excludesfile = /Users/cweagans/.gitignore_global
[github]
	user = cweagans
[branch]
	autosetuprebase = always
[filter "media"]
	clean = git media clean %f
	smudge = git media smudge %f
	required = true
[alias]
	amend = commit --amend --no-edit
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[rebase]
	autostash = true
[hooks]
	global = /Users/cweagans/.dotfiles/githooks
