defaults
logfile ~/.msmtp.log
tls_trust_file /etc/ssl/certs/ca-certificates.crt

account Personal
  host smtp.fastmail.com
  port 465
  protocol smtp
  auth on
  from me@cweagans.net
  user cweagans@fastmail.com
  passwordeval "~/.offlineimap.py get Personal"
  tls on
  tls_starttls off

account Mobomo
  host smtp.gmail.com
  port 587
  protocol smtp
  auth on
  from cameron@mobomo.com
  user cameron@mobomo.com
  passwordeval "~/.offlineimap.py get Mobomo"
  tls on

account Nasa
  host smtp01.ndc.nasa.gov
  port 587
  protocol smtp
  auth on
  from cameron.w.eagans@nasa.gov
  user ndc\ceagans
  passwordeval "~/.offlineimap.py get Nasa"
  tls on

account default : Personal
