defaults
logfile ~/.cache/mail/msmtp.log

account Gmail
    host smtp.gmail.com
    port 587
    tls on
    protocol smtp
    auth on
    from cweagans@gmail.com
    user cweagans@gmail.com
    passwordeval "secret-tool lookup mail Gmail"

